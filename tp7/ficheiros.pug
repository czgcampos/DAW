extends layout
block content
	.w3-container
		.w3-card-4
			header.w3-container.w3-teal
				h3 Lista de ficheiros
			.w3-container
				table.w3-table-all
					caption 
						h3 Lista de ficheiros
					tr
						th Nome
						th Link
					each f in lista
						tr
							td= f.nome
							td 
								a(href=f.link)=f.link
			footer.w3-container.w3-teal
				address Gerado po FileFormApp:
					a(href='/') Voltar à página principal